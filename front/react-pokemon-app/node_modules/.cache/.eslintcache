\r\n            })\r\n            .then(response => response.json())\r\n            .catch(error => this.handleError(error));\r\n    }\r\n    static addPokemon(pokemon: Pokemon): Promise<Pokemon> {\r\n        delete pokemon.created;\r\n\r\n        return fetch(`http://localhost:3001/pokemons`,{\r\n            method: 'POST',\r\n            body: JSON.stringify(pokemon),\r\n            headers: {'Content-Type':'application.json'}\r\n        })\r\n            .then(response => response.json())\r\n            .catch(error => this.handleError(error));\r\n    }\r\n\r\n\r\n    static handleError(error:Error): void {\r\n        console.error(error);\r\n    }\r\n}","E:\\Dev\\react-training\\front\\react-pokemon-app\\src\\models\\pokemon.ts",[],"E:\\Dev\\react-training\\front\\react-pokemon-app\\src\\helpers\\format-date.ts",[],"E:\\Dev\\react-training\\front\\react-pokemon-app\\src\\helpers\\format-type.ts",[],"E:\\Dev\\react-training\\front\\react-pokemon-app\\src\\components\\pokemon-card.tsx",[],"E:\\Dev\\react-training\\front\\react-pokemon-app\\src\\components\\pokemon-form.tsx",["60"],{"ruleId":"61","severity":1,"message":"62","line":1,"column":16,"nodeType":"63","messageId":"64","endLine":1,"endColumn":33},{"ruleId":"61","severity":1,"message":"65","line":1,"column":35,"nodeType":"63","messageId":"64","endLine":1,"endColumn":59},{"ruleId":"61","severity":1,"message":"66","line":2,"column":8,"nodeType":"63","messageId":"64","endLine":2,"endColumn":19},{"ruleId":"61","severity":1,"message":"67","line":1,"column":35,"nodeType":"63","messageId":"64","endLine":1,"endColumn":44},"@typescript-eslint/no-unused-vars","'FunctionComponent' is defined but never used.","Identifier","unusedVar","'FunctionComponentElement' is defined but never used.","'pokemonEdit' is defined but never used.","'useEffect' is defined but never used."]